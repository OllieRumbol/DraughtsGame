MoveTo.width) {\r\n                        counters[counterToMove.height][counterToMove.width] = 5;\r\n                        counters[squareToMoveTo.height][squareToMoveTo.width] = counterToMove.state;\r\n\r\n                        setCounters(counters);\r\n                        setSquares(renderSquares());\r\n                    }\r\n                }\r\n            }\r\n            //Player 1 taking player 2\r\n            \r\n            setCounterToMove(null);\r\n            setSquareToMoveTo(null);\r\n        }\r\n    }, [squareToMoveTo, counterToMove, counters])\r\n\r\n    return (\r\n        <div className=\"container fluid d-flex justify-content-center\">\r\n            <table>\r\n                <tbody className=\"border border-dark\">\r\n                    {squares}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    )\r\n}","C:\\Users\\otgbo\\Documents\\Repo\\draughtsgame\\src\\components\\Square.js",[],"C:\\Users\\otgbo\\Documents\\Repo\\draughtsgame\\src\\components\\Counter.js",[],"C:\\Users\\otgbo\\Documents\\Repo\\draughtsgame\\src\\components\\shared\\DisplayCard.js",[],"C:\\Users\\otgbo\\Documents\\Repo\\draughtsgame\\src\\components\\Players.js",[],"C:\\Users\\otgbo\\Documents\\Repo\\draughtsgame\\src\\components\\Pieces.js",[],"C:\\Users\\otgbo\\Documents\\Repo\\draughtsgame\\src\\components\\PiecesTaken.js",[],{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","severity":1,"message":"48","line":84,"column":8,"nodeType":"49","endLine":84,"endColumn":49,"suggestions":"50"},"no-native-reassign",["51"],"no-negated-in-lhs",["52"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'renderSquares'. Either include it or remove the dependency array.","ArrayExpression",["53"],"no-global-assign","no-unsafe-negation",{"desc":"54","fix":"55"},"Update the dependencies array to be: [squareToMoveTo, counterToMove, counters, renderSquares]",{"range":"56","text":"57"},[3501,